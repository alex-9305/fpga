#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55582482b1d0 .scope module, "count_up_16_bit_range_check_tb" "count_up_16_bit_range_check_tb" 2 4;
 .timescale -6 -9;
v0x55582483e7b0_0 .var "clk", 0 0;
v0x55582483e870_0 .var "enable", 0 0;
v0x55582483e940_0 .var "n_rst", 0 0;
S_0x55582482b360 .scope module, "dut" "count_up_16_bit_range_check" 2 9, 3 1 0, S_0x55582482b1d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "n_rst";
    .port_info 2 /INPUT 1 "enable";
    .port_info 3 /OUTPUT 16 "count_out";
v0x5558247efcf0_0 .net "clk", 0 0, v0x55582483e7b0_0;  1 drivers
v0x55582483e4c0_0 .var "count_out", 15 0;
v0x55582483e5a0_0 .net "enable", 0 0, v0x55582483e870_0;  1 drivers
v0x55582483e640_0 .net "n_rst", 0 0, v0x55582483e940_0;  1 drivers
E_0x555824828c90/0 .event negedge, v0x55582483e640_0;
E_0x555824828c90/1 .event posedge, v0x55582483e5a0_0, v0x5558247efcf0_0;
E_0x555824828c90 .event/or E_0x555824828c90/0, E_0x555824828c90/1;
    .scope S_0x55582482b360;
T_0 ;
    %wait E_0x555824828c90;
    %load/vec4 v0x55582483e5a0_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x55582483e4c0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55582483e640_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x55582483e4c0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55582483e4c0_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x55582483e4c0_0, 0;
    %load/vec4 v0x55582483e4c0_0;
    %cmpi/u 60000, 0, 16;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.4, 5;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x55582483e4c0_0, 0;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55582482b1d0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55582483e940_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55582483e7b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55582483e870_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55582482b1d0;
T_2 ;
    %vpi_call 2 17 "$dumpfile", "count_up_16_bit_range_check.vcd" {0 0 0};
    %vpi_call 2 18 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55582482b1d0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55582482b1d0;
T_3 ;
T_3.0 ;
    %delay 1000, 0;
    %load/vec4 v0x55582483e7b0_0;
    %inv;
    %store/vec4 v0x55582483e7b0_0, 0, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x55582482b1d0;
T_4 ;
    %delay 56000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55582483e940_0, 0;
    %delay 200000000, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55582483e940_0, 0;
    %delay 22000, 0;
    %vpi_call 2 34 "$display", "Test Complete" {0 0 0};
    %vpi_call 2 35 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
